<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>org.redis.redis</string>
	<key>ProgramArguments</key>
	<array>
        <string>bash</string>
        <string>-lc</string>
		<string>redis-server --daemonize yes</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
